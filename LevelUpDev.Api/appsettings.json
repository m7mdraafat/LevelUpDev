{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "CosmosDb": {
    "AccountEndpoint": "https://your-account.documents.azure.com:443/",
    "AccountKey": "your-key",
    "DatabaseName": "LevelUpDb",
    "Containers": {
      "Users": "users",
      "Stats": "stats",
      "Leaderboards": "leaderboards"
    }
  },
  "Jwt": {
    "Secret": "your-super-secret-key-at-least-32-chars",
    "Issuer": "LevelUpDev",
    "Audience": "LevelUpDev",
    "ExpirationMinutes": 10080
  },
  "GitHub": {
    "ClientId": "your-github-client-id",
    "ClientSecret": "your-github-client-secret"
  },
  "LeetCodeApi": {
    "BaseUrl": "https://alfa-leetcode-api.onrender.com"
  }
}
